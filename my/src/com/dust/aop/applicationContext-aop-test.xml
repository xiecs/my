<?xml version="1.0" encoding="UTF-8" ?>
<beans default-lazy-init="true"
	xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dwr="http://www.directwebremoting.org/schema/spring-dwr"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
                http://www.directwebremoting.org/schema/spring-dwr http://www.directwebremoting.org/schema/spring-dwr-2.0.xsd">

	<bean id="beforeAdvice" class="com.dust.aop.BeforeAdvice"></bean>
	<bean id="afterAdvice" class="com.dust.aop.AfterAdvice"></bean>
	<bean id="compareInterceptor" class="com.dust.aop.CompareInterceptor"></bean>
	<bean id="studenttarget" class="com.dust.aop.StudentImpl"></bean>

	<bean id="student" class="org.springframework.aop.framework.ProxyFactoryBean">
		<property name="proxyInterfaces">
			<value>com.dust.aop.IStudent</value>
		</property>
		<property name="interceptorNames">
			<list>
				<value>beforeAdvice</value>
				<value>afterAdvice</value>
				<value>compareInterceptor</value>
			</list>
		</property>
		<property name="target">
			<ref bean="studenttarget" />
		</property>

	</bean>

</beans>
